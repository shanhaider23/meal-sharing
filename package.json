    "babel-loader": "^8.1.0",
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.18.3",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.4",
    "css-loader": "^5.0.0",
    "debug": "~2.6.0",
    "dotenv": "^4.0.0",
    "dotenv-webpack": "^1.7.0",
    "express": "^4.16.4",
